{
  "manifest_version": 2,
  "name": "ICS to GCal",
  "short_name": "ICS2GCal",
  "version": "0.1.1",

  "description": "Import .ics files into Google Calendar with just two clicks",

  "author": "Fabian Henneke",
  "key": "",
  "background": {
    "persistent": false,
    "scripts": [
      "ical.min.js",
      "chrome-promise.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "snackbar.html"
  ],
  "permissions": [
    "contextMenus",
    "identity",
    "activeTab",
    "https://www.googleapis.com/*",
    "<all_urls>"
  ],
  "oauth2":   {
    "client_id": "205438406793-rren6k6no1q4vgkvebg9kbft8o4srjvm.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}
